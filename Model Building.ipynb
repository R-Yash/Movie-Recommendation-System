   "source": [
    "def GetDirector(obj):\n",
    "    gen = []\n",
    "    for i in ast.literal_eval(obj):\n",
    "        if i['job'] == 'Director':\n",
    "            gen.append(i['name'])\n",
    "            break\n",
    "    return gen    "
   ]
  },
