   "id": "d7a107e8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def GetActors(obj):\n",
    "    gen = []\n",
    "    counter = 0\n",
    "    for i in ast.literal_eval(obj):\n",
    "        if counter != 4:\n",
    "            gen.append(i['name'])\n",
    "            counter+=1\n",
    "        else:\n",
    "            break\n",
    "    return gen    "
   ]
  },
